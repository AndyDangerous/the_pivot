<h1>You can edit this category below</h1>
<%= form_for @category, url: administrator_category_path(@category), method: :patch do |f| %>

  <div class='field' id='categorytitle'>
    <%= f.label :title %><br/>
    <%= f.text_field :title %>
  </div>
  <div class='field' id='categorydescription'>
    <%= f.label :description %><br/>
    <%= f.text_area :description, size: "30x12" %>
  </div>
  <div class='actions'>
    <%= f.submit "Update this Category" %>
  </div>
<% end %>
