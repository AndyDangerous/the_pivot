<h1>Our menu</h1>
<hr/>
<table>
  <% @categories.each do |category| %>
  <tr>
    <td><%= category.title %></td>
    <td><%= category.description %></td>
    <td><%= link_to 'Edit', edit_administrator_category_path(category) %></td>
    <td><%= link_to 'Delete', category, method: :delete, data: {confirm: 'Are you sure?'} %></td>
  </tr>
  <% end %>
</table>
<%= link_to 'Create New Category', new_administrator_category_path %>
