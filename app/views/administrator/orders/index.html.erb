<%= link_to 'Paid Orders', administrator_paid_path, class: 'btn' %>
<%= link_to 'Ordered Orders', administrator_ordered_path, class: 'btn' %>
<%= link_to 'Cancelled Orders', administrator_cancelled_path, class: 'btn' %>
<%= link_to 'Completed Orders', administrator_completed_path, class: 'btn' %>

<% @orders.each do |order| %>

  <%= order.id %>
  <%= order.user.first_name %> <%= order.user.last_name %>
  <%= order.receiving %>
  <%= order.status %>
  <%= link_to "Order Details", administrator_order_path(order), class: 'btn' %>

<% end %>

