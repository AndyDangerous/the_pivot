
<div class="container">
<% provide(:title, "Signin") %>

<%= form_for :session, url: sessions_path do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Signin" %>
<% end %>

<%= link_to "Register now!", register_path %>
</div>
